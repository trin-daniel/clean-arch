{
  "name": "clean-node-api",
  "version": "1.8.0",
  "description": "application developed during clean architecture training",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9090 --nolazy ./dist/main/server.js",
    "test": "jest --passWithNoTests --runInBand",
    "test:unit": "jest --passWithNoTests --watch --runInBand -c test-unit.js",
    "test:integration": "jest --passWithNoTests --watch --runInBand -c test-integration.js",
    "test:staged": "jest --passWithNoTests --runInBand --findRelatedTests",
    "test:ci": "jest --passWithNoTests --runInBand --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.8",
    "@types/express-serve-static-core": "^4.17.12",
    "@types/jest": "^26.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.27",
    "@types/node": "^14.10.1",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-commit-msg-linter": "^2.6.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "mockdate": "^3.0.2",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.2",
    "nodemon": "^2.0.4",
    "swagger-ui-express": "^4.1.4",
    "validator": "^13.1.1"
  },
  "engines": {
    "node": "12.x"
  },
  "_moduleAliases": {
    "@infra": "dist/infra",
    "@domain": "dist/domain",
    "@main": "dist/main",
    "@presentation": "dist/presentation",
    "@validation": "dist/validation",
    "@data": "dist/data"
  }
}
